{
	"name": "New York Times Byline Restorer",
	"description": "Restores author bylines to the New York Times homepage",
	"manifest_version": 2,
	"version": "1.0.4",
	"content_scripts": [
		{
			"matches": ["https://www.nytimes.com/"],
			"js": ["bylines.js"],
			"css": ["style.css"],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"https://*.nytimes.com/*"
	]
}
